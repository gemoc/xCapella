					bindable := theMainBlock.elements[name = getNameOrUID(self.oclAsType(EObject))+'_fire']->first();
					_abstract := seqOfAbstractEntity[name = 'TOT_fire']->first();
				};
		
		theMainBlock.relations += result;
		}
		

