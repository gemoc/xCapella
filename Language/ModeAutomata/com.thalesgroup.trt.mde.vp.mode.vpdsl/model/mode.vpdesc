      <VP_Classes_Associations xsi:type="vpdesc:ExternalClassAssociation" name="exitActions" cardinality="0..*">
        <ExternalTarget href="../../com.thalesgroup.trt.mde.vp.al/models/al.ecore#//Action"/>
      </VP_Classes_Associations>
      <VP_Classes_Associations xsi:type="vpdesc:ExternalClassAssociation" name="availableFunctionalChains" cardinality="0..*" type="reference">
        <ExternalTarget href="http://www.polarsys.org/capella/core/fa/1.3.0#//FunctionalChain"/>
      </VP_Classes_Associations>
